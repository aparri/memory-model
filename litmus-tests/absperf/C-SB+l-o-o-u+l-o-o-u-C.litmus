P0(int *sl, int *x0, int *x1)
{
	int r2;
	int r1;

	r2 = cmpxchg_acquire(sl, 0, 1);
	WRITE_ONCE(*x0, 1);
	r1 = READ_ONCE(*x1);
	smp_store_release(sl, 0);
}

P1(int *sl, int *x0, int *x1)
{
	int r2;
	int r1;

	r2 = cmpxchg_acquire(sl, 0, 1);
	WRITE_ONCE(*x1, 1);
	r1 = READ_ONCE(*x0);
	smp_store_release(sl, 0);
}

filter (0:r2=0 /\ 1:r2=0)
exists (0:r1=0 /\ 1:r1=0)
