P0(spinlock_t *sl, int *x0, int *x1)
{
	int r1;

	spin_lock(sl);
	WRITE_ONCE(*x0, 1);
	r1 = READ_ONCE(*x1);
	spin_unlock(sl);
}

P1(spinlock_t *sl, int *x0, int *x1)
{
	int r1;

	spin_lock(sl);
	WRITE_ONCE(*x1, 1);
	r1 = READ_ONCE(*x0);
	spin_unlock(sl);
}

